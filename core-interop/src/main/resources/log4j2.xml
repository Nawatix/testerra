<?xml version="1.0" encoding="UTF-8" ?>
<Configuration packages="eu.tsystems.mms.tic.testframework.logging">
    <Appenders>
        <Console name="CONSOLE">
            <PatternLayout pattern="%d{dd.MM.yyyy HH:mm:ss.SSS} [%t][%p] %testerra: %c{2} - %m%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="CONSOLE"/>
        </Root>
    </Loggers>
</Configuration>
