<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="GuiElement" verbose="10" configfailurepolicy="continue" thread-count="10" parallel="false">

    <test name="GuiElement Core Features" parallel="methods">
        <groups>
            <run>
                <exclude name="SEQUENTIAL"/>
            </run>
        </groups>
        <classes>
            <class name="eu.tsystems.mms.tic.testframework.test.core.guielement.GuiElementStandardTests"/>
            <class name="eu.tsystems.mms.tic.testframework.test.core.guielement.GuiElementFilterTest"/>
            <class name="eu.tsystems.mms.tic.testframework.test.core.guielement.GuiElementAdditionalTests"/>
            <class name="eu.tsystems.mms.tic.testframework.test.core.guielement.GuiElementExceptionsTest"/>
            <class name="eu.tsystems.mms.tic.testframework.test.core.guielement.GuiElementListTests"/>
            <class name="eu.tsystems.mms.tic.testframework.test.core.guielement.GuiElementShadowRootTest"/>
        </classes>
    </test>

    <test name="GuiElement Layout Features" parallel="methods">
        <groups>
            <run>
                <exclude name="SEQUENTIAL"/>
            </run>
        </groups>
        <packages>
            <package name="eu.tsystems.mms.tic.testframework.core.test.layoutcheck"/>
        </packages>
    </test>

</suite>
