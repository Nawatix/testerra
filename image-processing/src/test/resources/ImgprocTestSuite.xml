<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Fennec Imgproc Unit Test Suite" verbose="10" configfailurepolicy="continue" thread-count="10"
       parallel="false">

    <listeners>
        <listener class-name="eu.tsystems.mms.tic.testframework.report.FennecListener"/>
    </listeners>

    <test name="Imgproc Templatematching Featurebased Matcher Test">
        <classes>
            <class name="eu.tsystems.mms.tic.testframework.imgproc.templatematching.matchers.featurebased.test.MultiPatternMultiTemplateTest"/>
            <class name="eu.tsystems.mms.tic.testframework.imgproc.templatematching.matchers.featurebased.test.EndToEndTest"/>
        </classes>
    </test>
</suite>